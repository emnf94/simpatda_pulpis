            t_jumlah9: $('#t_jumlah9').val(),
            t_jumlah10: $('#t_jumlah10').val()
        }, function(data) {
            var aa = JSON.parse(data);
            $('#t_dasarpengenaan').val(aa.t_dasarpengenaan);
            $('#t_jmlhpajak').val(aa.t_jmlhpajak);
            $('#t_jmlhkenaikan').val(aa.t_jmlhkenaikan);
